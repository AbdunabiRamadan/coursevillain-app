{
  "name": "coursevillain",
  "author": "Embry-Riddle Aeronautical University",
  "contributors": [
    "Jack Grant",
    "Tim",
    "Tyler Carr",
    "Leonard Brunotte"
  ],
  "description": "CourseVillain is a tool for professors and students at ERAU to use if they are running a course or are building a course to scrape the Course Hero website for ERAU content of a specific course and automate submission of the copyright infringement form.",
  "version": "0.1.0",
  "main": "app.js",
  "engines": {
    "node": "8.9.4"
  },
  "scripts": {
    "start": "electron .",
    "pack:macos": "electron-packager . CourseVillain --out=../CVDist --platform=darwin --arch=x64 --icon=buildResources/mac/icon.icns --overwrite",
    "pack:win32": "electron-packager . CourseVillain --out=../CVDist --platform=win32 --arch=ia32 --icon=buildResources/win/icon.ico --overwrite",
    "pack:win64": "electron-packager . CourseVillain --out=../CVDist --platform=win32 --arch=x64 --icon=buildResources/win/icon.ico --overwrite",
    "build": "npm run pack:macos && npm run pack:win32 && npm run pack:win64"
  },
  "build": {
    "appId": "com.coursevillain",
    "productName": "CourseVillain",
    "copyright": "Copyright Â© 2020 CourseVillain",
    "mac": {
      "category": "public.coursevillain"
    },
    "protocols": {
      "name": "coursevillain-protocol",
      "schemes": [
        "coursevillain"
      ]
    }
  },
  "dependencies": {
    "moment": "^2.27.0",
    "puppeteer-firefox": "^0.5.1"
  },
  "license": "MIT",
  "devDependencies": {
    "electron": "^9.0.5",
    "electron-packager": "^15.0.0"
  }
}
